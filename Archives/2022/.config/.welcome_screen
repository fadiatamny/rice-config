#!/bin/bash

art=(
    "/home/fadi/.asciArt/awesome"
    "/home/fadi/.asciArt/cat"
    "/home/fadi/.asciArt/fancy"
    "/home/fadi/.asciArt/lex"
    "/home/fadi/.asciArt/sleepyDoge"
    "/home/fadi/.asciArt/snorlax"
    "/home/fadi/.asciArt/taco"
